{
  "name": "factorio-manager-server",
  "private": true,
  "version": "0.1.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "NODE_ENV=production npm run build && node dist",
    "dev": "NODE_ENV=development nodemon -x babel-node --inspect src",
    "build": "babel -d dist src",
    "test": "npm run lint && npm run test:integration && npm run test:unit",
    "test:unit": "NODE_ENV=test jest",
    "test:unit:debug": "NODE_ENV=test node --inspect node_modules/.bin/jest --watchAll -i",
    "test:integration": "NODE_ENV=test jest --forceExit --detectOpenHandles -ic jest.integration.config.js",
    "test:integration:debug": "NODE_ENV=test node --inspect-brk node_modules/.bin/jest --forceExit --detectOpenHandles --watchAll -ic jest.integration.config.js",
    "lint": "eslint src migrations knexfile.js *.config.js jest.*",
    "clean": "./scripts/clean.sh"
  },
  "author": "Matthew Palmer <mspalmer91@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "@babel/cli": "^7.11.6",
    "@babel/node": "^7.10.5",
    "@babel/preset-env": "^7.11.5",
