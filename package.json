    "graphql": "^15.3.0",
    "graphql-scalars": "^1.2.7",
    "graphql-tag": "^2.11.0",
    "knex": "^0.21.6",
    "knex-stringcase": "^1.4.3",
    "luxon": "^1.25.0",
    "mkdirp": "^1.0.4",
    "rmfr": "^2.0.0",
    "semver": "^7.3.2",
    "snakecase-keys": "^3.2.0",
    "sqlite3": "^5.0.3"
